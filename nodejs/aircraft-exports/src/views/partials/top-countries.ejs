<table>
    <thead>
        <tr>
            <th>To</th>
            <th>Value</th>
            <th>Date</th>
            <th>Last Updated</th>
            <th>Link</th>
        </tr>
    </thead>
    <tbody>
        <% country.forEach(item=> { %>
            <tr>
                <td>
                    <%= item.country2 %>
                </td>
                <td>
                    <%= item.value %>
                </td>
                <td>
                    <%= item.date %>
                </td>
                <td>
                    <%= item.lastupdate %>
                </td>
                <td>
                    <%= item.url %>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>