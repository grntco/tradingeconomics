<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Aircraft, Spacecraft Exports</title>
</head>

<body>
    <header></header>
    <h1>Aircraft, Spacecraft Exports</h1>
    <footer></footer>
</body>
<table>
    <thead>
        <tr>
            <th>To</th>
            <th>Value</th>
            <th>Date</th>
            <th>Last Updated</th>
            <th>Link</th>
        </tr>
    </thead>
    <tbody>
        <% country1Data.forEach(item=> { %>
            <tr>
                <td>
                    <%= item.country2 %>
                </td>
                <td>
                    <%= item.value %>
                </td>
                <td>
                    <%= item.date %>
                </td>
                <td>
                    <%= item.lastupdate %>
                </td>
                <td>
                    <%= item.url %>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>

</html>